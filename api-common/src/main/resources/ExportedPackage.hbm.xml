<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.metadatasharing">
	<class name="ExportedPackage" table="metadatasharing_exported_package">
		<id name="exportedPackageId" type="int" column="exported_package_id" unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="exported_package_id" insert="false" />
		<property name="uuid" type="java.lang.String" column="uuid"  length="38" unique="true" />
		<property name="dateCreated" type="java.util.Date" column="date_created" not-null="true" length="19" />
		<property name="name" type="java.lang.String" column="name" length="64" not-null="true" />
		<property name="description" type="java.lang.String" column="description" length="256" not-null="true" />
		<property name="groupUuid" type="java.lang.String" column="group_uuid" length="38" not-null="true" />
		<property name="version" type="java.lang.Integer" column="version" not-null="true" />
		<property name="content" type="java.sql.Blob" column="content" not-null="false" lazy="true" />
		<property name="published" type="java.lang.Boolean" column="published" not-null="true" />
	</class>

	<class name="ExportedPackageSummary" table="metadatasharing_exported_package" mutable="false">
		<id name="exportedPackageId" type="int" column="exported_package_id" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="uuid" type="java.lang.String" column="uuid"  length="38" unique="true" />
		<property name="dateCreated" type="java.util.Date" column="date_created" not-null="true" length="19" />
		<property name="name" type="java.lang.String" column="name" length="64" not-null="true" />
		<property name="description" type="java.lang.String" column="description" length="256" not-null="true" />
		<property name="groupUuid" type="java.lang.String" column="group_uuid" length="38" not-null="true" />
		<property name="version" type="java.lang.Integer" column="version" not-null="true" />
		<property name="published" type="java.lang.Boolean" column="published" not-null="true" />
	</class>
</hibernate-mapping>
